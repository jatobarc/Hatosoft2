<!-- Default table -->
            <div class="panel panel-default">
                <div class="panel-heading"><h6 class="panel-title"><i class="icon-table2"></i> Default table</h6></div>
                <div class="table-responsive">
                  <table class="table">
                      <thead>
                          <tr>
                              <th>#</th>
                              <th>Animal</th>
                              <th>Mes nutricion</th>
                              <th colspan="3"></th>
                          </tr>
                      </thead>
                      <tbody>
                        <% @nutriciones.each do |nutricion| %>
                          <tr>
                            <td><%= nutricion.animal.id %></td>
                            <td><%= nutricion.animal.nombre %></td>
                            <td><%= nutricion.mes_nutricion %></td>
                            <td><%= link_to 'Show', nutricion %></td>
                            <td><%= link_to 'Edit', edit_nutricion_path(nutricion) %></td>
                            <td><%= link_to 'Destroy', nutricion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                          </tr>
                        <% end %>
                      </tbody>
                  </table>
                </div>
          </div>
          <!-- /default table -->






