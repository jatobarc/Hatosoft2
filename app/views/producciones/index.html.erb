
        <!-- Default table -->
            <div class="panel panel-default">
                <div class="panel-heading"><h6 class="panel-title"><i class="icon-table2"></i> Default table</h6></div>
                <div class="table-responsive">
                  <table class="table">
                      <thead>
                          <tr>
                              <th>#</th>
                              <th>Animal</th>
                              <th>Mes produccion</th>
                              <th colspan="3"></th>
                          </tr>
                      </thead>
                      <tbody>
                        <% @producciones.each do |produccion| %>
                          <tr>
                            <td><%= produccion.animal.id %></td>
                            <td><%= produccion.animal.nombre %></td>
                            <td><%= produccion.dia_produccion %></td>
                            <td><%= link_to 'Show', produccion %></td>
                            <td><%= link_to 'Edit', edit_produccion_path(produccion) %></td>
                            <td><%= link_to 'Destroy', produccion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                          </tr>
                        <% end %>
                      </tbody>
                  </table>
                </div>
          </div>
          <!-- /default table -->









