


<!-- Default table -->
            <div class="panel panel-default">
                <div class="panel-heading"><h6 class="panel-title"><i class="icon-table2"></i> Default table</h6></div>
                <div class="table-responsive">
                  <table class="table">
                      <thead>
                          <tr>
                              <th>#</th>
                              <th>Fecha</th>
                              <th>Codigo</th>
                              <th>Elemento</th>
                              <th>Material</th>
                              <th>Sustancia aseo</th>
                              <th>Sustancia desinfectante</th>
                              <th>Uso</th>
                              <th>Dosis</th>
                              <th>Higiene manana</th>
                              <th>Higiene tarde</th>
                              <th colspan="3"></th>
                          </tr>
                      </thead>
                      <tbody>
                          
                        <% @higienes.each do |higiene| %>
                          <tr>
                            <td><%= higiene.id %></td>
                            <td><%= higiene.fecha %></td>
                            <td><%= higiene.codigo %></td>
                            <td><%= higiene.elemento %></td>
                            <td><%= higiene.material %></td>
                            <td><%= higiene.sustancia_aseo %></td>
                            <td><%= higiene.sustancia_desinfectante %></td>
                            <td><%= higiene.uso %></td>
                            <td><%= higiene.dosis %></td>
                            <td><%= higiene.higiene_manana %></td>
                            <td><%= higiene.higiene_tarde %></td>
                            <td><%= link_to 'Show', higiene %></td>
                            <td><%= link_to 'Edit', edit_higiene_path(higiene) %></td>
                            <td><%= link_to 'Destroy', higiene, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                          </tr>
                        <% end %>
                          
                      </tbody>
                  </table>
                </div>
          </div>
          <!-- /default table -->





<%= link_to 'New Higiene', new_higiene_path, :class => "btn btn-lg btn-info" %>
