<!-- Default table -->
            <div class="panel panel-default">
                <div class="panel-heading"><h6 class="panel-title"><i class="icon-table2"></i> Default table</h6></div>
                <div class="table-responsive">
                  <table class="table">
                      <thead>
                          <tr>
                              <th>#</th>
                              <th>Nombre</th>
                              <th>Madre</th>
                              <th>Padre</th>
                              <th>Fecha Parto</th>
                              <th>Codigo nacido</th>
                              <th colspan="3"></th>
                          </tr>
                      </thead>
                      <tbody>
                        <% @partos.each do |parto| %>
                          <tr>
                          <!--<td><%= parto.animal.id %></td>-->
                            <td><%= parto.id %></td>
                            <td><%= parto.nombre %></td>
                            <td><%= parto.animal.nombre %></td>
                            <!--<td><%= parto.padre_id %></td>-->
                            <td><%= Animal.find(parto.padre_id).nombre %></td>
                            

                            <td><%= parto.fecha_parto %></td>
                            <td><%= parto.codigo_nacido %></td>
                            <td><%= link_to 'Show', parto %></td>
                            <td><%= link_to 'Edit', edit_parto_path(parto) %></td>
                            <td><%= link_to 'Destroy', parto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                          </tr>
                        <% end %>
                      </tbody>
                  </table>
                </div>
          </div>
          <!-- /default table -->

<%= link_to 'New Parto', new_parto_path, :class => "btn btn-lg btn-info" %>
