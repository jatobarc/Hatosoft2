<!-- Default table -->
            <div class="panel panel-default">
                <div class="panel-heading"><h6 class="panel-title"><i class="icon-table2"></i> Default table</h6></div>
                <div class="table-responsive">
                  <table class="table">
                      <thead>
                          <tr>
                              <th>#</th>
                              <th>Codigo</th>
                              <th>Area</th>
                              <th>Tipo pasto</th>
                              <th colspan="3"></th>
                          </tr>
                      </thead>
                      <tbody>

                      <% @parcelas.each do |parcela| %>
                        <tr>
                          <td><%= parcela.id %></td>
                          <td><%= parcela.codigo %></td>
                          <td><%= parcela.area %></td>
                          <td><%= parcela.tipo_pasto %></td>
                          <td><%= link_to 'Show', parcela %></td>
                          <td><%= link_to 'Edit', edit_parcela_path(parcela) %></td>
                          <td><%= link_to 'Destroy', parcela, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                        </tr>
                      <% end %>
                         
                      </tbody>
                  </table>
                </div>
          </div>
          <!-- /default table -->






<%= link_to 'New Parcela', new_parcela_path, :class => "btn btn-lg btn-info" %>
